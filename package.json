{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "./src/entry.ts",
  "scripts": {
    "mock": "cross-env NODE_ENV=development APP_ENV=mock webpack serve --config ./config/webpack.config.js",
    "dev": "cross-env NODE_ENV=development APP_ENV=dev webpack serve --config ./config/webpack.config.js",
    "stage": "cross-env NODE_ENV=development APP_ENV=stage webpack serve --config ./config/webpack.config.js",
    "gray": "cross-env NODE_ENV=development APP_ENV=gray webpack serve --config ./config/webpack.config.js",
    "prod": "cross-env NODE_ENV=development APP_ENV=prod webpack serve --config ./config/webpack.config.js",
    "build:dev": "cross-env NODE_ENV=production APP_ENV=dev webpack --config ./config/webpack.config.js",
    "build:stage": "cross-env NODE_ENV=production APP_ENV=stage webpack --config ./config/webpack.config.js",
    "build:gray": "cross-env NODE_ENV=production APP_ENV=gray webpack --config ./config/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production APP_ENV=prod webpack --config ./config/webpack.config.js",
    "build": "pnpm build:prod",
    "addfile": "pnpm plop --plopfile ./scripts/plop/index.js",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "author": "runnan",
  "license": "ISC",
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.24",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.5",
    "mini-css-extract-plugin": "^2.6.0",
    "plop": "^3.0.5",
    "postcss": "^8.4.8",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.5.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-css-modules": "^3.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^8.0.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "antd": "^4.19.1",
    "axios": "^0.26.0",
    "i18next": "^21.6.14",
    "immer": "^9.0.12",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.15.6",
    "react-is": "^18.1.0",
    "react-router-dom": "^6.2.2",
    "styled-components": "^5.3.3",
    "unstated-next": "^1.1.0",
    "use-immer": "^0.7.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
