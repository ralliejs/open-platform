{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "./src/entry.ts",
  "scripts": {
    "mock": "cross-env NODE_ENV=development APP_ENV=mock webpack serve --config ./config/webpack.config.js",
    "dev": "cross-env NODE_ENV=development APP_ENV=dev webpack serve --config ./config/webpack.config.js",
    "stage": "cross-env NODE_ENV=development APP_ENV=stage webpack serve --config ./config/webpack.config.js",
    "gray": "cross-env NODE_ENV=development APP_ENV=gray webpack serve --config ./config/webpack.config.js",
    "prod": "cross-env NODE_ENV=development APP_ENV=prod webpack serve --config ./config/webpack.config.js",
    "build:dev": "cross-env NODE_ENV=production APP_ENV=dev webpack --config ./config/webpack.config.js",
    "build:stage": "cross-env NODE_ENV=production APP_ENV=stage webpack --config ./config/webpack.config.js",
    "build:gray": "cross-env NODE_ENV=production APP_ENV=gray webpack --config ./config/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production APP_ENV=prod webpack --config ./config/webpack.config.js",
    "build": "pnpm build:prod",
    "addfile": "pnpm plop --plopfile ./scripts/plop/index.js",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "author": "runnan",
  "license": "ISC",
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.1.1",
    "mini-css-extract-plugin": "^2.7.2",
    "plop": "^3.1.2",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.0.2",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.8.4",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "antd": "^5.2.0",
    "axios": "^1.3.2",
    "i18next": "^22.4.9",
    "immer": "^9.0.19",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "styled-components": "^5.3.6",
    "unstated-next": "^1.1.0",
    "use-immer": "^0.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
